particle_system Particle/Tornado
{
    material        Particle/Tornado
    quota           1500
    cull_each       false
    sorted          true
    local_space     false
    billboard_type  point

    particle_width  6
    particle_height 6

    // Emit from the top, falling down
    emitter Point
    {
        emission_rate   200
        time_to_live    5

        position        0 6.6 0
        direction       0 -1 0
        angle           12

        velocity_min    5.0
        velocity_max    7.0

        // PU color ramp start-ish
        colour_range_start  1 0.9 0.7 1
        colour_range_end    1 0.45 0.45 0.9
    }

    // Strong downward pull (keeps column tight)
    affector LinearForce
    {
        force_vector      0 -3 0
        force_application add
    }

    // Fake the vortex: random lateral direction changes
    affector DirectionRandomiser
    {
        randomness  1.2
    }

    // Visual swirl by rotating sprites
    affector Rotator
    {
        rotation_range_start            0
        rotation_range_end              360
        rotation_speed_range_start     -180
        rotation_speed_range_end        180
    }

    // Fade out over lifetime (approx PU colour end to transparent)
    affector ColourFader
    {
        red    -0.4
        green  -0.6
        blue   -0.7
        alpha  -0.9
    }
}

particle_system Particle/TornadoSystem
{
    material        Particle/Tornado
    particle_width  7.5
    particle_height 7.5
    cull_each       false
    quota           3000
    billboard_type  oriented_self
    sorted          true
    local_space     false

    // PU Circle(radius 0.6) approximation: tiny box emission area
    emitter Box
    {
        angle           10
        emission_rate   150
        time_to_live    4

        // PU velocity 18
        velocity_min    18
        velocity_max    18

        // Small emission region roughly like a ring origin
        width           1.2
        height          0.1
        depth           1.2

        // Start shooting mostly upwards (PU didn’t specify direction; tornado implies up)
        direction       0 1 0
        position        0 0 0
    }

    // Fake "vortex": upward pull + slight sideways drift + random direction jitter
    affector LinearForce
    {
        force_vector        0 6 0
        force_application   add
    }

    // A subtle sideways force helps give it “lean / swirl” motion
    affector LinearForce
    {
        force_vector        1.2 0 0
        force_application   add
    }

    // Swirl approximation (since no real Vortex affector): continuously perturbs velocity direction
    affector DirectionRandomiser
    {
        randomness  0.9
    }

    // PU colour: 0 -> (1,0.95,0.95,1), 0.5 -> alpha 0
    affector ColourInterpolator
    {
        time0   0.0
        colour0 1 0.95 0.95 1

        time1   0.5
        colour1 1 0.95 0.95 0

        time2   1.0
        colour2 1 0.95 0.95 0
    }

    // PU Scale random 1.5..10.5 not available directly here
    // If you want the “random size” look, use TWO stacked systems with different sizes (see note below).
}

