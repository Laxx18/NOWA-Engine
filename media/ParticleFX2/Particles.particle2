particle_system Particle/TimeShift_Watch
{
    material        Particle/Watch
    quota           64
    cull_each       false
    sorted          true
    local_space     true
    billboard_type  point

    // We’ll drive per-particle sizing via ranges below (if your build supports it).
    // Otherwise keep these as the “default” size.
    particle_width  6
    particle_height 6

    emitter Point
    {
        // PU emission_rate curve: 0 -> 10 at t=0.2 -> 0 at t=3
        // Approximation: short burst early.
        emission_rate   50
        duration        0.2
        repeat_delay    3.0

        // If you want a single-shot effect (no looping), remove repeat_delay
        // and stop the system via code after ~3s.

        // Lifetime: choose something that matches the overall 3s envelope
        time_to_live_min  2.0
        time_to_live_max  3.0

        velocity_min    0.9
        velocity_max    3.0

        // Approx random size 3..12 (PU all_particle_dimensions)
        particle_width_min   3.0
        particle_width_max   12.0
        particle_height_min  3.0
        particle_height_max  12.0

        // Base colour (PU emitter colour)
        colour          1 0.88 0.26 1
    }

    // PU multiply fade: invisible -> visible -> invisible
    // Implemented as alpha ramp (keeping RGB same)
    affector ColourInterpolator
    {
        time0   0.0
        colour0 1 0.88 0.26 0

        time1   0.5
        colour1 1 0.88 0.26 1

        time2   1.0
        colour2 1 0.88 0.26 0
    }

    // PU TextureRotator (random rotation speed -2..2)
    affector Rotator
    {
        rotation_range_start          0
        rotation_range_end            360
        rotation_speed_range_start    -2
        rotation_speed_range_end       2
    }
}

