        scale0  0.3

        time1   0.4
        scale1  2.4

        time2   1.0
        scale2  0.0
    }

    // PU TextureRotator: rot random 1..40, speed 0.5
    affector Rotator
    {
        rotation_range_start           1
        rotation_range_end             40
        rotation_speed_range_start     0.5
        rotation_speed_range_end       0.5
    }

    // PU LinearForce exclude PointEmitter: force_vector 0 -3 0
    // ParticleFX2 doesn't reliably support exclude_emitter on all affectors,
    // so we apply it globally (close enough visually).
    affector LinearForce
    {
        force_vector        0 -3 0
        force_application   add
    }
}

