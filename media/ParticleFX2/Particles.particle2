    quota           2000
    cull_each       false
    billboard_type  point
    sorted          true
    local_space     true

    // default_particle_width/height = 0.3
    // (Already in meters-like units in your script, so keep as-is)
    particle_width  0.30
    particle_height 0.30

    // SphereSurface radius 3
    // ParticleFX2 doesn't have SphereSurface here -> approximate with a small box around origin
    emitter Box
    {
        emission_rate   100
        duration        2
        time_to_live    4

        position        0 0 0

        // Outward burst approximation (auto_direction)
        angle           180
        direction       0 1 0

        // velocity 0 but auto_direction; give a tiny velocity so the burst expands
        velocity_min    0.05
        velocity_max    0.20

        // Radius 3 -> box diameter 6
        width           6
        height          6
        depth           6
    }

    // Colour time_colour:
    // 0: red, 0.6: cyan, 1: blue
    affector ColourInterpolator
    {
        time0   0.0
        colour0 1 0 0 1

        time1   0.6
        colour1 0 1 1 1

        time2   1.0
        colour2 0 0 1 1
    }

    // Scale xyz_scale 0.3 (grows)
    affector Scaler
    {
        rate 0.08   // tune: 0.04..0.15 depending on how bloomy you want it
    }

    // Optional: fade near end so it doesn’t pop
    affector ColourFader
    {
        alpha -0.35
    }

    // Optional: "jet after 2s" approximation.
    // ParticleFX2 can't enable an affector mid-flight via script,
    // so we approximate a late push by a constant mild force (subtle).
    // If you want a stronger kick, increase this.
    affector LinearForce
    {
        force_vector      0 0.15 0
        force_application add
    }
}

