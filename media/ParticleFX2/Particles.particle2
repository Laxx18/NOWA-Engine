particle_system Particle/WindyFireSystem
{
    // PU: material ParticleUniverse/Fire
    // Pick your closest ParticleFX2 fire material:
    material        Particle/Fire_01

    // PU doesn’t specify quota; pick something safe.
    quota           1200
    cull_each       false
    sorted          true
    local_space     true

    // Fire usually looks better when it can rotate freely
    billboard_type  oriented_self

    // PU: default_particle_width/height = 1.8
    particle_width  1.8
    particle_height 1.8

    // --- PU Point emitter (wind toggling) approximation ---
    // PU had observers toggling wind left/right globally.
    // ParticleFX2 doesn’t have PU observers, so we approximate by:
    //  - using TWO point emitters with slight left/right bias
    //  - adding DirectionRandomiser for turbulent wind
    emitter Point
    {
        emission_rate   50
        angle           30

        // ttl dyn_random 3..6
        time_to_live_min 3
        time_to_live_max 6

        // vel dyn_random 3..4.2
        velocity_min    3
        velocity_max    4.2

        // slight wind to the right
        direction       0.35 1 0
    }

    emitter Point
    {
        emission_rate   50
        angle           30

        time_to_live_min 3
        time_to_live_max 6

        velocity_min    3
        velocity_max    4.2

        // slight wind to the left
        direction       -0.35 1 0
    }

    // --- PU Scale xyz_scale -0.15 (shrink over lifetime) ---
    affector Scaler
    {
        rate -0.15
    }

    // --- “wind switching” look: turbulence per particle ---
    affector DirectionRandomiser
    {
        // increase if you want more sideways wobble
        randomness  0.8
    }

    // --- PU Colour: (1,1,1,1) -> (0,0,0,1) ---
    // Approx as fading RGB down (no alpha fade).
    affector ColourFader
    {
        red   -0.25
        green -0.25
        blue  -0.25
        // alpha 0
    }
}

