particle_system Particle/Beam2
{
    material        Particle/Beam_2        // Use your existing beam material
    quota           400                    // enough for continuity
    cull_each       false
    sorted          true
    local_space     true

    // Beam look: thin width, long height, stretched along velocity
    particle_width  0.15                   //  default 0.3 * 0.5
    particle_height 0.30                   //  default 0.6 * 0.5

    billboard_type  oriented_self
    // Optional (if you want it to "start" at emitter point):
    // billboard_origin top_center

    emitter Point
    {
        emission_rate   50                 // emission_rate 50
        angle           25                 // angle 25

        // time_to_live 40 is extremely long; for a beam, short TTL looks better.
        // This gives a stable continuous beam with high emission.
        time_to_live    0.35

        // velocity 6 with scale_velocity 0.5 => 3
        direction       0 1 0              // direction is implicit; upward fits many beam examples
        velocity_min    2.8
        velocity_max    3.2

        // emitter position 0 6 0
        position        0 6 0

        // duration 2 seconds
        duration        2
        repeat_delay    0                  // loops; set large value if you want "fire once"
    }

    // Gravity { gravity 500 } (point gravity) -> approximate with a downward force.
    // Tune this: increase magnitude for stronger drop/arc.
    affector LinearForce
    {
        force_vector      0 -5 0
        force_application add
    }

    // Optional: fade beam segments for smoother look (often helps)
    affector ColourFader
    {
        alpha -2.5
    }

    // Optional flicker / jitter (similar vibe to beam energy)
    affector Rotator
    {
        rotation_speed_range_start  -90
        rotation_speed_range_end     90
        rotation_range_start          0
        rotation_range_end           360
    }
}

// Technique 1 approximation: "Flare_04" core particles that brighten and shrink.
// ParticleFX2 has no point gravity, so we fake "collapse" via shrinking + mild pull.
particle_system Particle/BlackHole_Core
{
    material        Particle/Flare_04
    particle_width  0.12          // 12 * 0.01
    particle_height 0.12
    cull_each       false
    quota           2500
    billboard_type  point
    sorted          true

    // emitter: SphereSurface radius 12
    // ParticleFX2 doesn't have SphereSurface, so we approximate with a box volume
    // roughly matching the diameter (24 -> 0.24).
    emitter Box
    {
        angle           180
        emission_rate   200
        time_to_live    2.0        // didn't specify; tune 1.5..4.0
        position        0 0 0
        direction       0 1 0
        velocity_min    0.02       // velocity 3 * 0.01 = 0.03 (slightly reduced)
        velocity_max    0.04

        width           0.24
        height          0.24
        depth           0.24
    }

    // Colour: dark -> bright -> white (time_colour)
    affector ColourInterpolator
    {
        time0   0.0
        colour0 0 0 0.2 1

        time1   0.9
        colour1 0.8 0.8 1 1

        time2   1.0
        colour2 1 1 1 1
    }

    // "Collapse": shrink over lifetime (xyz_scale -4.5).
    // ParticleFX2 scaler is uniform. Tune this aggressively for the black-hole pull feel.
    affector Scaler
    {
        rate -0.06       // start here; try -0.04..-0.12 depending on look
    }

    // Fake gravity/pull (no point gravity available): slight downward pull so it feels like sucked in.
    affector LinearForce
    {
        force_vector      0 -0.25 0   // gravity 2700 * 0.01 = 27 (too strong visually). Tune 0.1..1.0
        force_application add
    }
}


